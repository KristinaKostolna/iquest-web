    <div class="circles">
        <div class="bg-point4 circle index1 orange" data-aos="zoom-in"></div>
        <div class="bg-point5 circle index2 orange" data-aos="zoom-in"></div>
        <div class="bg-point1 circle index1 yellow" data-aos="zoom-in"></div>
        <div class="bg-point2 circle index2 yellow" data-aos="zoom-in"></div>
        <div class="bg-point3 circle index3 yellow" data-aos="zoom-in"></div>
        <div class="bg-point7 circle index1 blue" data-aos="zoom-in"></div>
        <div class="bg-point8 circle index2 blue" data-aos="zoom-in"></div>
        <div class="bg-point9 circle index3 blue" data-aos="zoom-in"></div>
        <div class="bg-point10 circle index1 green" data-aos="zoom-in"></div>
	</div>
    <div class="row">
        <div class="col-40">
            <h1 class="h1-volne" data-aos="fade-right">Volné pozice:</h1>
            <img alt="volné pozice v iQuest s.r.o." src="/images/volne_pozice.jpg" class="volne-img" data-aos="fade-right">
        </div>
        <div class="col-offset-10 col-35 volne-text">
